SOURCES=ModworksSDK.cpp Utility.cpp CurlWrapper.cpp MinizipWrapper.cpp Mod.cpp minizip/unzip.cpp minizip/ioapi.cpp
OBJS=ModworksSDK.obj Utility.obj CurlWrapper.obj MinizipWrapper.obj Mod.obj unzip.obj ioapi.obj

foo: ModworksSDK.cpp
 cl /EHsc /MD /LD $(SOURCES) /I ../include /link ../lib/libcurl.a ../lib/libcurldll.a ../lib/zlib.lib ../lib/zdll.lib ../lib/pthreadVC2.lib /SUBSYSTEM:CONSOLE
 lib.exe /OUT:ModworksSDK.lib $(OBJS)